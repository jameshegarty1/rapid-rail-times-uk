{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "baseUrl": "src",
    "paths": {
      "*": ["*"]
    },
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}

